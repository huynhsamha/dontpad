{
  "name": "dontpad",
  "version": "1.1.0",
  "homepage": "https://dontpad.herokuapp.com/",
  "private": true,
  "description": "Advanced Dontpad - Editor online without identity",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huynhsamha/dontpad.git"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-css": "node-sass-chokidar src/ -o src/",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "crypto-js": "^4.0.0",
    "node-rsa": "^1.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-froala-wysiwyg": "^2.9.8",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "4.0.1",
    "react-share": "^2.2.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "socket.io-client": "^2.1.0"
  },
  "devDependencies": {
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2"
  },
  "proxy": "http://localhost:4200",
  "author": {
    "name": "huynhsamha",
    "email": "huynhsamha@gmail.com",
    "url": "https://github.com/huynhsamha"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/huynhsamha/dontpad/issues"
  },
  "engines": {
    "node": ">=10.23.0",
    "npm": ">=6.14.8"
  },
  "contributors": [
    "huynhsamha"
  ],
  "icon": "./public/favicon.ico",
  "keywords": [
    "dontpad",
    "advanced dontpad",
    "docs",
    "react",
    "heroku",
    "mlab",
    "mongodb",
    "mongo",
    "socket.io",
    "scoket",
    "link",
    "image",
    "video",
    "url",
    "format",
    "realtime"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
