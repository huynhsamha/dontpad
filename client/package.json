{
  "name": "dontpad",
  "version": "1.0.1",
  "homepage": "https://dontpad.herokuapp.com/",
  "private": true,
  "description": "Advanced Dontpad - Editor online without identity",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huynhsamha/dontpad.git"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-css": "node-sass-chokidar src/ -o src/",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-froala-wysiwyg": "^2.7.6",
    "react-reveal": "^1.2.2",
    "react-scripts": "1.1.0",
    "react-share": "^2.2.0",
    "socket.io-client": "^2.1.0"
  },
  "devDependencies": {
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2"
  },
  "proxy": "http://localhost:4200",
  "author": {
    "name": "huynhsamha",
    "email": "huynhsamha@gmail.com",
    "url": "https://github.com/huynhsamha"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/huynhsamha/dontpad/issues"
  },
  "engines": {
    "node": ">=6.10.0",
    "npm": ">=3.10.8"
  },
  "contributors": [
    "huynhsamha"
  ],
  "icon": "./public/favicon.ico",
  "keywords": [
    "dontpad",
    "advanced dontpad",
    "docs",
    "react",
    "heroku",
    "mlab",
    "mongodb",
    "mongo",
    "socket.io",
    "scoket",
    "link",
    "image",
    "video",
    "url",
    "format",
    "realtime"
  ]
}
